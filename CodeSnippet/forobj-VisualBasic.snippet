	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>for loop tile</Title>
			<Author>Xuyuwtu</Author>
			<Shortcut>fortile</Shortcut>
		</Header>
		<Snippet>
			<References>
				<Reference>
					<Assembly>OTAPI.dll</Assembly>
				</Reference>
			</References>
			<Imports>
				<Import>
					<Namespace>Terraria</Namespace>
				</Import>
			</Imports>
			<Declarations>
				<Literal>
					<ID>LoopVarX</ID>
					<Default>x</Default>
				</Literal>
				<Literal>
					<ID>LoopVarY</ID>
					<Default>y</Default>
				</Literal>
				<Literal>
					<ID>ObjectVar</ID>
					<Default>tile</Default>
				</Literal>
			</Declarations>
			<Code Language="VB" Kind="method body">
				<![CDATA[For $LoopVarY$ = 0 To Main.maxTilesY - 1
					For $LoopVarX$ = 0 To Main.maxTilesX - 1
						Dim $ObjectVar$ = Main.tile($LoopVarX$, $LoopVarY$)
					Next
				Next]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>for loop npc</Title>
			<Author>Xuyuwtu</Author>
			<Shortcut>fornpc</Shortcut>
		</Header>
		<Snippet>
			<References>
				<Reference>
					<Assembly>OTAPI.dll</Assembly>
				</Reference>
			</References>
			<Imports>
				<Import>
					<Namespace>Terraria</Namespace>
				</Import>
			</Imports>
			<Declarations>
				<Literal>
					<ID>LoopVar</ID>
					<Default>i</Default>
				</Literal>
				<Literal>
					<ID>ObjectVar</ID>
					<Default>npc</Default>
				</Literal>
			</Declarations>
			<Code Language="VB" Kind="method body">
				<![CDATA[For $LoopVar$ = 0 To Main.maxNPCs - 1
					Dim $ObjectVar$ = Main.npc($LoopVar$)
				Next]]>
			</Code>
		</Snippet>
	</CodeSnippet>

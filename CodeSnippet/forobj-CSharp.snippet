	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>for loop tile</Title>
			<Author>Xuyuwtu</Author>
			<Shortcut>fortile</Shortcut>
		</Header>
		<Snippet>
			<References>
				<Reference>
					<Assembly>OTAPI.dll</Assembly>
				</Reference>
			</References>
			<Imports>
				<Import>
					<Namespace>Terraria</Namespace>
				</Import>
			</Imports>
			<Declarations>
				<Literal>
					<ID>LoopVarX</ID>
					<Default>x</Default>
				</Literal>
				<Literal>
					<ID>LoopVarY</ID>
					<Default>y</Default>
				</Literal>
				<Literal>
					<ID>ObjectVar</ID>
					<Default>tile</Default>
				</Literal>
			</Declarations>
			<Code Language="CSharp" Kind="method body">
				<![CDATA[for (int $LoopVarY$ = 0; $LoopVarY$ < Main.maxTilesY; $LoopVarY$++)
				{
					for (int $LoopVarX$ = 0; $LoopVarX$ < Main.maxTilesX; $LoopVarX$++)
					{
						var $ObjectVar$ = Main.tile[$LoopVarX$, $LoopVarY$];
					}
				}]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>